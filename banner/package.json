    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "sass": "^1.80.6",
    "sass-embedded": "^1.93.2",
    "terser": "^5.44.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.10",
    "vite-css-prefixer": "^1.0.4"
