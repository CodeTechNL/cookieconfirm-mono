    "format": "npx --yes prettier . --write",
    "format:check": "npx --yes prettier . --check"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
